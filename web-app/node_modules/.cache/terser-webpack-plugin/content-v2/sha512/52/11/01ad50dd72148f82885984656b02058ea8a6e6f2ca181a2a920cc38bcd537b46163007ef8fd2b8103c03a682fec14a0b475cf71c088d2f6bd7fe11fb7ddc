\r\\n                            />\\r\\n                            <TextField\\r\\n                                id=\\\"new-product-descr\\\"\\r\\n                                name='surname'\\r\\n                                label=\\\"Nazwisko\\\"\\r\\n                                onBlur={this.newUserStateChange}\\r\\n                                margin=\\\"normal\\\"\\r\\n                                fullWidth\\r\\n                                multiline\\r\\n                            >\\r\\n                            </TextField>\\r\\n                            <TextField\\r\\n                                autoFocus\\r\\n                                id=\\\"new-product-shop\\\"\\r\\n                                name='login'\\r\\n                                label=\\\"Login\\\"\\r\\n                                onBlur={this.newUserStateChange}\\r\\n                                margin=\\\"normal\\\"\\r\\n                                style={{\\r\\n                                    width: '400px'\\r\\n                                }}\\r\\n                            />\\r\\n                            <TextField\\r\\n                                autoFocus\\r\\n                                id=\\\"new-product-category\\\"\\r\\n                                name='password'\\r\\n                                label=\\\"Hasło\\\"\\r\\n                                onBlur={this.newProductStateChange}\\r\\n                                type=\\\"password\\\"\\r\\n                                margin=\\\"normal\\\"\\r\\n                                style={{\\r\\n                                    width: '400px'\\r\\n                                }}\\r\\n                            />\\r\\n                            <TextField\\r\\n                                autoFocus\\r\\n                                id=\\\"new-product-price\\\"\\r\\n                                name='password'\\r\\n              